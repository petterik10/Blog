<%- include("partials/header"); -%>
<h1>Culture Posts</h1>
<%  posts.forEach(function(post){ %>
<div class="container">
  <div class="row">
    <div class="col-sm">
      <h2><%=post.title%></h2>
      <h4> <%= `${post.firstName} ${post.lastname} `%> </h4>
      <h4> <%=`${post.time} `%> </h4>
      <p>
        <a href="/culture/<%=post._id%>">Read</a>
      </p>
      <% }) %>
    </div>
  </div>
</div>
